{
  "name": "express-vue-crud",
  "version": "0.0.0",
  "description": "Crud NodeJS con VueJS 2",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "dev": "webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-validator": "^3.2.1",
    "jade": "~1.11.0",
    "knex": "^0.13.0",
    "morgan": "~1.10.0",
    "mysql": "^2.14.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "laravel-mix": "^1.4.2",
    "nodemon": "^1.11.0"
  },
  "author": "Ismael López Caamaño",
  "license": "MIT"
}
